{
  "name": "react-fmt-input",
  "version": "1.4.0",
  "description": "React formatted input component",
  "keywords": [
    "typescript",
    "react",
    "mask",
    "input",
    "formatted"
  ],
  "source": "src/react-fmt-input.tsx",
  "module": "lib/react-fmt-input.es.js",
  "types": "lib/types.d.ts",
  "license": "MIT",
  "repository": "https://github.com/kobzarvs/react-formatted-input.git",
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@parcel/compressor-brotli": "^2.0.0",
    "@parcel/compressor-gzip": "^2.0.0",
    "@parcel/optimizer-esbuild": "^2.0.0",
    "@parcel/optimizer-terser": "^2.0.0",
    "@parcel/packager-ts": "^2.0.0",
    "@parcel/reporter-bundle-analyzer": "^2.0.0",
    "@parcel/transformer-typescript-tsc": "^2.0.0",
    "@parcel/transformer-typescript-types": "^2.0.0",
    "@parcel/validator-typescript": "^2.0.0",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "parcel": "^2.0.0",
    "parcel-optimizer-custom-terser": "1.0.2",
    "rimraf": "^3.0.2",
    "terser": "^5.9.0",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "yarn clear && cross-env NODE_ENV=production parcel build",
    "build:dev": "yarn clear && parcel build",
    "clear": "rimraf dist lib .parcel-cache parcel-bundle-reports"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "cross-env": "^7.0.3"
  }
}
